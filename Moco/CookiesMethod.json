[
  {
    "description":"这是我们的第一个mock例子",
    "request":{
      "uri":"/demo"
    },
    "response":{
      "text":"第一个moco框架demo",
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      }
    }
  },
  {
    "description":"这是一个访问成功后，返回cookies的示例",
    "request":{
      "uri":"/getcookie",
      "method":"get"
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"
      },
      "text":"登陆成功",
      "cookies":{
        "authentication":"true"

      }
    }
  },
  {
    "description":"这是一个需要携带cookies才能访问成功的Get请求",
    "request":{
      "uri":"/getwithcookie",
      "method":"get",
      "cookies":{
        "authentication":"true"
      }
    },
    "response":{
      "headers":{
        "Content-Type":"text/html;charset=gbk"},
      "text":"访问成功!"
    }
  },
  {
    "description":"这是一个需要携带cookies才能访问成功的Post请求",
    "request":{
      "uri":"/postwithcookie",
      "method":"post",
      "json":{
        "name":"pangluo",
        "age":"18"
      },
      "cookies":{
        "authentication":"true"
      }
    },
    "response":{
      "status":"200",
      "json":{
        "succes":"true",
        "response":"OK"
      }
    }
  }
]